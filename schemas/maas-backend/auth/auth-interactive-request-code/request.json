{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://api.maas.global/maas-backend/auth/auth-interactive-request-code/request",
  "description": "Request schema for auth-interactive-request-code",
  "type": "object",
  "properties": {
    "payload": {
      "type": "object",
      "properties": {
        "phone": {
          "$ref": "../../../core/components/customer.json#/properties/rawPhone"
        },
        "name": {
          "description": "Operator name",
          "type": "string",
          "minLength": 4,
          "maxLength": 160
        },
        "token": {
          "description": "Encryption token for the key",
          "type": "string",
          "minLength": 80,
          "maxLength": 1024
        }
      },
      "required": ["phone", "name"],
      "additionalProperties": false
    },
    "headers": {
      "$ref": "../../../core/components/api-common.json#/definitions/headers"
    }
  },
  "required": ["payload"],
  "additionalProperties": false
}
